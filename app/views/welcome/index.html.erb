<% if current_user %>
  <p>Currently logged in as: <%= current_user.email %></p>
<% end %>

<%= button_to "Create New User", register_path, method: :get %>
<%= link_to "Log in", login_form_path %>

<section class="existing-users">
  <p>Existing Users:</p>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to user.email, user_path(user.id) %></li>
    <% end %>
  </ul>
</section>